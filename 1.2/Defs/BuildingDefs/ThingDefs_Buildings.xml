<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <DesignatorDropdownGroupDef>
    <defName>DDGDTest</defName>
  </DesignatorDropdownGroupDef>
  <ThingDef ParentName="BuildingBase">
    <thingClass>Building</thingClass>
    <defName>AMN_WallFlag</defName>
    <label>flag on a wall</label>
    <description>A flag on a wall.</description>
    <graphicData>
      <texPath>Things/FlagPole/FlagOnWall</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>1,1</drawSize>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <thingCategories>
      <li>Buildings</li>
    </thingCategories>
    <castEdgeShadows>false</castEdgeShadows>
    <socialPropernessMatters>false</socialPropernessMatters>
    <fillPercent>0.05</fillPercent>
    <statBases>
      <Mass>1</Mass>
      <MaxHitPoints>20</MaxHitPoints>
      <WorkToMake>100</WorkToMake>
      <Flammability>1.0</Flammability>
      <Beauty>10</Beauty>
    </statBases>
    <size>(1,1)</size>
    <placeWorkers>
      <li>Amnabi.PlaceWorker_WallFlag</li>
    </placeWorkers>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <comps>
      <li Class="Amnabi.CompProperties_Flag">
        <offsetX>0</offsetX>
        <offsetY>0</offsetY>
        <scaleX>1.0</scaleX>
        <scaleY>1.0</scaleY>
        <drawOnWall>true</drawOnWall>
      </li>
    </comps>
    <costStuffCount>4</costStuffCount>
    <costList>
      <Cloth>40</Cloth>
    </costList>
    <building>
      <isEdifice>false</isEdifice>
    </building>
    <pathCost>50</pathCost>
    <passability>Standable</passability>
    <designationCategory>Furniture</designationCategory>
    <designatorDropdown>DDGDTest</designatorDropdown>
    <uiIconScale>0.3</uiIconScale>
    <clearBuildingArea>false</clearBuildingArea>
  </ThingDef>
